<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studentsityapp">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.studentsityapp.FacLeaveApprovalActivity"></activity>
        <activity android:name="com.example.studentsityapp.PreviousRecordActivity" />
        <activity
            android:name="com.example.studentsityapp.LeaveformActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.example.studentsityapp.StartupPage" />
        <activity android:name="com.example.studentsityapp.SignupActivity" />
        <activity android:name="com.example.studentsityapp.LoginActivity" />
        <activity android:name="com.example.studentsityapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>